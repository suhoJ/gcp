{
  "manifest_version": 2,
  "name": "Webcam Capture",
  "description": "Capture images from the webcam every few seconds",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "activeTab"
  ]
}

